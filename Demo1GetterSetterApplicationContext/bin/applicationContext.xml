<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns = "http://www.springframework.org/schema/beans" 
xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">


<bean id="emp" class="com.Employee">

	<property name="uid"><value>1</value></property>
	<property name="name"><value>Prashant</value></property>
	<property name="desig"><value>Developer</value></property>

</bean>

<bean id="product" class="com.Product">

	<property name="pid"><value>1</value></property>
	<property name="pname"><value>Apple</value></property>
	<property name="pdesc"><value>iPhone</value></property>
	<property name="price"><value>60000</value></property>

</bean>
	
</beans>