<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns = "http://www.springframework.org/schema/beans" 
xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

<bean id="emp" class="com.Employee">

	<constructor-arg name="uid"><value>1</value></constructor-arg>
	<constructor-arg name="name"><value>Prashant</value></constructor-arg>
	<constructor-arg name="age"><value>23</value></constructor-arg>
	<constructor-arg name="salary"><value>50000</value></constructor-arg>
	<constructor-arg name="desig"><value>Developer</value></constructor-arg>
	
</bean>

<bean id="product" class="com.Product" scope="prototype">

	<constructor-arg name="pid"><value>1</value></constructor-arg>
	<constructor-arg name="pname"><value>Apple</value></constructor-arg>
	<constructor-arg name="pdesc"><value>iPhone</value></constructor-arg>
	<constructor-arg name="price"><value>60000</value></constructor-arg>

</bean>

<bean id="student" class="com.Student">
    <property name="sid"><value>1</value></property>
    <property name="sname"><value>Prashant</value></property>
    <property name="smarks"><value>90</value></property>
    <property name="ssection"><value>A</value></property>
</bean>

	
</beans>