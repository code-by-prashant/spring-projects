<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="person" class="com.Person">
        <property name="pid" value="1" />
        <property name="name" value="Prashant" />
        
        <property name="hobbies">
            <list>
                <value>Reading</value>
                <value>Music</value>
                <value>Traveling</value>
            </list>
        </property>
        
        <property name="email">
            <list>
                <value>prashant@gmail.com</value>
                <value>xyz@gmail.com</value>
                <value>abc@gmail.com</value>
            </list>
        </property>
        
        <property name="phone">
            <set>
                <value>1234567890</value>
                <value>9876543210</value>
            </set>
        </property>
        
        <property name="education">
            <map>
                <entry key="Post Graduation" value="ABC University" />
                <entry key="Graduation" value="ABC University" />
                <entry key="School" value="XYZ School" />
            </map>
        </property>
    </bean>

</beans>
